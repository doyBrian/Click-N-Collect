n    for (let i = array.length - 1; i > 0; i--) {\\n      let j = Math.floor(Math.random() * (i + 1)); // random index from 0 to i\\n      [array[i], array[j]] = [array[j], array[i]]; // swap elements\\n    }\\n\\n    this.setState({images: array})\\n  }\\n\\n\\n\\n  // Map over this.state.images and render image on the page\\n  render() {\\n    return (\\n      <div class=\\\"container\\\">\\n        <Jumbotron/>\\n        <Title>Number of Shirts: {this.state.shirts} | Total: $ {this.state.total}.00 </Title>\\n        <Wrapper>\\n        {this.state.images.map(picture => (\\n          <ImageCard\\n            handleClick={this.handleClick}\\n            id = {picture.id}\\n            image = {picture.image}\\n            counter = {this.state.counter}\\n          />\\n        ))}\\n        </Wrapper>\\n        </div>\\n    );\\n  }\\n}\\n\\nexport default App;\\n\",\"import React from \\\"react\\\";\\nimport ReactDOM from \\\"react-dom\\\";\\nimport App from \\\"./App\\\";\\nimport \\\"./index.css\\\";\\n\\nReactDOM.render(<App />, document.getElementById(\\\"root\\\"));\\n\"]}","code":"(window.webpackJsonp=window.webpackJsonp||[]).push([[0],[,,,,,,function(e){e.exports=[{id:1,image:\"https://images.express.com/is/image/expressfashion/0020_00303473_0302?cache=on&wid=361&fmt=jpeg&qlt=75,1&resmode=sharp2&op_usm=1,1,5,0&defaultImage=Photo-Coming-Soon\",clicked:!1},{id:2,image:\"https://images.express.com/is/image/expressfashion/0020_00303655_0746?cache=on&wid=361&fmt=jpeg&qlt=75,1&resmode=sharp2&op_usm=1,1,5,0&defaultImage=Photo-Coming-Soon\",clicked:!1},{id:3,image:\"https://images.express.com/is/image/expressfashion/0020_00303656_1360?cache=on&wid=361&fmt=jpeg&qlt=75,1&resmode=sharp2&op_usm=1,1,5,0&defaultImage=Photo-Coming-Soon\",clicked:!1},{id:4,image:\"https://images.express.com/is/image/expressfashion/0020_06033240_1400?cache=on&wid=361&fmt=jpeg&qlt=75,1&resmode=sharp2&op_usm=1,1,5,0&defaultImage=Photo-Coming-Soon\",clicked:!1},{id:5,image:\"https://images.exp